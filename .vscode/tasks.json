{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"taskName": "Open File With Sublime",
			"command": "/Applications/'Sublime Text.app'/Contents/SharedSupport/bin/subl",
			"args": [
				"${file}"
			],
			"type": "shell",
			"showOutput": "never",
			"problemMatcher": []
		},
		{
			"taskName": "Open File With Emacs Client",
			"command": "emacsclient",
			"args": [
				"-n",
				"${file}"
			],
			"type": "shell",
			"showOutput": "never",
			"problemMatcher": []
		}
	]
}